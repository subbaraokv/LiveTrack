<ul class="breadcrumb">
    <li>
        <a href="/">Home</a> <span class="divider">/</span>
    </li>
    <li>
        <a href="/Tags">Tags</a> <span class="divider">/</span>
    </li>
    <li class="active">Create</li>
</ul>

<h1><%= title %></h1>
<div id="tag-list" class="k-content">
    <form class="form-vertical" method="post" action="/tag/create">
        <div class="control-group">
            <label class="control-label" for="name">Name</label>
            <div class="controls">
                <input id="name" class="input-xlarge span3" name="tag[name]" type="text">
            </div>
        </div>
        <div class="form-actions">
            <button class="btn btn-primary" type="submit">Save changes</button>
            <a class="btn" href="/Tags">Cancel</a>
        </div>
    </form>
</div>
